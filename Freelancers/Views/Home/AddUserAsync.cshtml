@model List<Users>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div class="modal-body">

    <table id="myTable" class="table table-bordered table-hover table-sm">
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Mail</th>
            <th>Phone</th>
            <th>Skills</th>
            <th>Hobby</th>

            <th>DELETE</th>
            <th>EDIT</th>
        </tr>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr style="font-size: 15px;">
                    <td>@item.username</td>
                    <td>@item.mail</td>
                    <td>@item.phonenumber</td>
                    <td>@item.skillSets</td>
                    <td>@item.hobby</td>

                    <td>@Html.ActionLink("Delete", "Delete", new { id = item.id })</td>
                    <td>@Html.ActionLink("Edit", "Edit", new { id = item.id, item.username, item.mail, item.phonenumber, item.skillSets, item.hobby })</td>

                </tr>
            }
        }
    </table>
</div>